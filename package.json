{
  "name": "personal-website",
  "version": "1.0.0",
  "description": "My personal website ludovic.cabre.me",
  "scripts": {
    "build": "webpack --config=webpack.config.js",
    "watch": "webpack --config=webpack.config.js --watch",
    "dev": "webpack-dev-server --host 0.0.0.0 --content-base dist",
    "clean": "rm -fr dist",
    "start": "forever start --minUptime 1000ms --spinSleepTime 1000ms --append --uid \"cabre\" server.js",
    "stop": "forever stop cabre",
    "restart": "forever restart cabre"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lud2k/personal-website.git"
  },
  "author": "Ludovic Cabre",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lud2k/personal-website/issues"
  },
  "homepage": "https://ludovic.cabre.me",
  "devDependencies": {
    "file-loader": "^5.0.2",
    "react-modal": "^3.11.1",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "@teamsupercell/typings-for-css-modules-loader": "^2.1.0",
    "@types/node": "^12.12.8",
    "@types/react": "^16.9.11",
    "awesome-typescript-loader": "^5.2.1",
    "css-loader": "^3.2.0",
    "express": "^4.17.1",
    "forever": "^2.0.0",
    "prettier": "^1.19.1",
    "raw-loader": "^3.1.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "serialize-javascript": ">=2.1.1",
    "style-loader": "^1.0.0",
    "typescript": "^3.7.2",
    "typings-for-css-modules-loader": "^1.7.0",
    "webpack": "^4.41.2",
    "webpack-dev-server": "^3.9.0"
  }
}
